package cn.bnsr.edu_yun.backstage.base.view;
/**
 * 订单统计视图
 * @author fangxiongwei
 * @date 2016年10月17日
 */
public class OrderStatsView {
	private String days;//日期
	private String year;//年
	private String month;//月
	private int orderNum;//订单数
	private double amountNum;//金额数
	public String getDays() {
		return days;
	}
	public void setDays(String days) {
		this.days = days;
	}
	public String getYear() {
		return year;
	}
	public void setYear(String year) {
		this.year = year;
	}
	public String getMonth() {
		return month;
	}
	public void setMonth(String month) {
		this.month = month;
	}
	public int getOrderNum() {
		return orderNum;
	}
	public void setOrderNum(int orderNum) {
		this.orderNum = orderNum;
	}
	public double getAmountNum() {
		return amountNum;
	}
	public void setAmountNum(double amountNum) {
		this.amountNum = amountNum;
	}
	
}
