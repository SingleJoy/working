<script type="text/javascript">
  $(document).ready(function(e){
    // 添加学员
    $(".show_pop").on("click",function(){
        $('#modal').show();
        $('.modal-open').append('<div class="modal-backdrop  in"></div>');
    })
    // 关闭弹窗
    $('.pop_close').click(function(){
        $('#modal').hide();
        $('#import_modal').hide();
        $('#letter_modal').hide();
        $('#data_modal').hide();
        $('#remark_modal').hide();
        $('.modal-backdrop').remove();
    })
    //  批量导入学员
    $(".import_student").on("click",function(){
        $('#import_modal').show();
        $('.modal-open').append('<div class="modal-backdrop  in"></div>');
    })
    // 发私信
    $(".letter_send").on("click",function(){
        $('#letter_modal').show();
        $('.modal-open').append('<div class="modal-backdrop  in"></div>');
    })
    //  查看资料
    $(".check_data").on("click",function(){
        $('#data_modal').show();
        $('.modal-open').append('<div class="modal-backdrop  in"></div>');
    })
    // 备注
    $(".remark_member").on("click",function(){
        $('#remark_modal').show();
        $('.modal-open').append('<div class="modal-backdrop  in"></div>');
    })
    // 移除
    $('.student-remove').click(function(){
        $(this).parent().parent().parent().parent().parent().remove();
    })
    //点击下拉框
    $('.btn-group .dropdown-toggle').click(function(e){
        e.stopPropagation();
        if ($(this).siblings(".dropdown-menu").css("display") == "none") {
            $(".dropdown-menu").hide();
            $(this).siblings(".dropdown-menu").show();
        }else{
            $(".dropdown-menu").hide();
        }
    })
    $(document).click(function(){
        $(".dropdown-menu").hide();
    })
  })
</script>
</body>
