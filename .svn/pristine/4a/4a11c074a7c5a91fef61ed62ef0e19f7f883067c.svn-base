  <select id="showLastMessage" resultType="cn.bnsr.edu_yun.frontstage.train.view.MessageView" parameterType="cn.bnsr.edu_yun.frontstage.train.view.MessageView">
  	SELECT m.id,m.title,m.type,u.username,m.is_read,DATE_FORMAT(m.release_time,'%Y-%m-%d') AS release_time 
  	FROM message_relation mr 
  	INNER JOIN message m 
  	ON mr.message_id = m.id AND mr.source_id=${source_id} AND mr.source_type = ${source_type} AND m.status=1 
  	INNER JOIN user u 
  	ON m.user_id=u.id 
  	ORDER BY m.release_time DESC 
  	LIMIT 0,1
  </select>
  
  <select id="queryClassNotices" resultType="cn.bnsr.edu_yun.frontstage.train.view.MessageView" parameterType="cn.bnsr.edu_yun.frontstage.train.view.MessageView">
  	SELECT m.id,m.title,DATE_FORMAT(m.release_time,'%Y-%m-%d %H:%i:%s') as release_time,u.username
  	FROM message m 
  	INNER JOIN message_relation mr 
  	ON mr.source_id=${source_id} 
  	AND mr.source_type=${source_type} 
  	AND mr.message_id=m.id 
  	AND m.type=5 
  	AND m.status=1
  	INNER JOIN user u
  	ON u.id=m.user_id 
  	ORDER BY m.id DESC
  	LIMIT ${startLine},${rows}
  </select>
  
