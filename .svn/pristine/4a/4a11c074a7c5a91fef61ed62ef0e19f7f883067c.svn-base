  <select id="showAllMessage"  resultType="cn.bnsr.edu_yun.frontstage.train.view.MessageView" parameterType="cn.bnsr.edu_yun.frontstage.train.view.MessageView">
    SELECT m.id,m.title,m.type,m.content,u.username,DATE_FORMAT(m.release_time,'%Y-%m-%d') AS release_time,m.status,mf.path as file_path,mf.file_name,
    DATE_FORMAT(m.release_time,'%d') AS release_date,DATE_FORMAT(m.release_time,'%m') AS release_moon
	FROM message m 
	LEFT JOIN `user` u ON m.user_id=u.id
	LEFT JOIN message_relation mr ON mr.message_id=m.id
	LEFT JOIN message_file mf ON mf.message_id=m.id
	WHERE mr.source_type=${source_type} AND mr.source_id=${source_id}
