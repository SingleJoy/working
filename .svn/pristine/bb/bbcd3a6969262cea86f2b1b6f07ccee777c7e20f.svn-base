	<select id="queryTeachingPlan" resultType="cn.bnsr.edu_yun.frontstage.resource.view.TeachingPlanView" parameterType="cn.bnsr.edu_yun.frontstage.resource.view.TeachingPlanView">
		SELECT tp.*,u.username,(CASE WHEN u.is_default_img=0 THEN (SELECT path FROM cert_model_picture WHERE type= 2 AND defaulted = 0 order by upload_time desc limit 1) ELSE u.icon END) AS icon,ui.true_name,ui.titles,sl.subjectName AS subject_name,sl1.subjectName AS stage_name,pl.versionName AS edition_name,
		bc1.bookCatelogName AS grade_name,bc2.bookCatelogName AS unit_name,bc3.bookCatelogName AS lesson_name
		FROM teaching_plan tp
		LEFT JOIN USER u ON u.id=tp.user_id
		LEFT JOIN user_info ui ON ui.user_id=tp.user_id
		LEFT JOIN subject_list sl ON sl.id=tp.subject
		LEFT JOIN subject_list sl1 ON sl.parent_id=sl1.id
		LEFT JOIN pubver_list pl ON pl.id=tp.edition
		LEFT JOIN book_cat bc1 ON bc1.id=tp.grade
		LEFT JOIN book_cat bc2 ON bc2.id=tp.unit
		LEFT JOIN book_cat bc3 ON bc3.id=tp.lesson
		WHERE tp.id=${id}
	</select>
