  <select id="findAll" parameterType="cn.bnsr.edu_yun.frontstage.train.view.StudentRelationView" resultType="cn.bnsr.edu_yun.frontstage.train.view.StudentRelationView">
    SELECT sr.id, sr.user_id, u.username, sr.role_type,
    ui.true_name,ui.school,ui.course,ui.sex,u.phone,sr.level,ui.titles
    <if test="from_type != 0">
    	,u.icon
    </if>
    FROM student_relation sr
    LEFT JOIN user u ON u.id=sr.user_id
    LEFT JOIN user_info ui ON ui.user_id=u.id
    <where>
	    <if test="source_id != null">
	       AND sr.source_id=${source_id}
	    </if>
	    <if test="source_type != null">
	       AND sr.source_type=${source_type}
	    </if>
	    <if test="user_id != null">
	       AND sr.user_id=${user_id}
	    </if>
	    <if test="username != null and username !='' ">
		      <![CDATA[AND  u.username LIKE '%${username}%']]>  
		</if>
    </where>
	<if test="from_type != 0">
	    ORDER BY role_type 
	</if>
    <if test="rows!=null and rows!=0">
	LIMIT ${startLine},${rows}
	</if>
 </select>
  
  <select id="exportStudent"  resultType="cn.bnsr.edu_yun.frontstage.train.view.StudentRelationView">
    SELECT <if test="source_type ==0">c.`name`</if><if test="source_type ==1">w.`name`</if>
    AS source_name,u.username,u.phone,u.email,sr.remarks,
    (CASE WHEN sr.role_type=0 THEN "管理员" WHEN sr.role_type=1 THEN "专家"
	WHEN sr.role_type=2 THEN "普通学员" WHEN sr.role_type=3 THEN "坊主" ELSE "" END) AS role_name
	FROM student_relation sr
	<if test="source_type == 0">
	 LEFT JOIN community c ON c.id=sr.source_id
	</if>
	<if test="source_type == 1">
	 LEFT JOIN workshop w ON w.id=sr.source_id
	</if>
	LEFT JOIN user u ON u.id=sr.user_id
	WHERE sr.source_id=${source_id} AND sr.source_type=${source_type}
	<if test="ids != null">
	   AND sr.id IN (${ids})
	</if>
    ORDER BY sr.id DESC
  </select>
  
  <select id="studentDataStats" resultType="cn.bnsr.edu_yun.frontstage.train.view.DataStatsView" parameterType="cn.bnsr.edu_yun.frontstage.train.view.DataStatsView">
  	SELECT DATE_FORMAT(sr.create_time, '%Y-%m-%d') days, COUNT(sr.id) AS newNum, t.countNum
	FROM student_relation sr
	LEFT JOIN (
		SELECT COUNT(*) AS countNum, s.source_id
		FROM student_relation s
		WHERE s.source_id = ${id} 
			AND s.source_type = ${dataType}
	) t ON t.source_id = sr.source_id
	WHERE sr.source_id = ${id}
		AND sr.source_type = ${dataType} 
		<if  test="monthType == 0 or monthType == 1 ">
			<![CDATA[AND sr.create_time LIKE  '${days}%']]>  
		</if>
		<if  test="monthType == 2 or monthType == 3 ">
		 	<![CDATA[ AND (sr.create_time >= '${startTime}' AND sr.create_time <= '${endTime}' )]]>  
		</if>
	GROUP BY days
  </select>
  
