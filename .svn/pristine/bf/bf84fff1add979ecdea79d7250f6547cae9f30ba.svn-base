.private_letter_main_content{margin-top: 10px;}
.private_letter_main_content>ul{}
.private_letter_main_content>ul>li{width: 650px;height:59px;border-bottom: 1px solid #ddd;margin-top: 15px;}
.private_letter_main_content>ul>li:last-child{margin-bottom:50px;}
.private_letter_main_content>ul>li>label{float: right;font-size: 14px;opacity: 0.8;margin-top: 30px;}
.private_letter_main_content>ul>li>label>a{display: none;}
.private_letter_main_content>ul>li>label>a>span{padding: 0 10px;}
.private_letter_main_content>ul>li>p{margin-left: 50px;margin-top: -40px;}
.private_letter_main_content>ul>li>a>img{width: 40px;height: 40px;border-radius: 50%;}
.private_letter_main_content>ul>li>p>a,.private_letter_main_content>ul>li>p>label{font-size: 14px;cursor: pointer;}
.private_letter_main_content>ul>li>p>span{font-size: 14px;color:#8a6d3b;}
.private_letter_main_content>ul>li>span{font-size: 14px;margin-left: 50px;opacity: 0.7;margin-top: -10px;position: absolute;}

