/*查看私信对话列表*/
#see_private_letter_list{min-height: 620px;}
#see_private_letter_list>a{float: right;margin-right: 50px;margin-top: -30px;}
.private_letter_list_dialog{}
.private_letter_list_dialog>ul{margin-top: 20px;margin-bottom: 50px;}
.private_letter_list_dialog>ul>li{width: 650px;height: 94px;margin-top: 10px;margin-left: 50px;}
.other_man{font-size: 14px;position: absolute;margin-top:25px;}
.mine{font-size: 14px;float: right;margin-top: -30px;}
.other_man>img,.mine>img{width: 40px;height: 40px;border-radius: 50%;}
.arrow{top:24px;left:-11px;position: relative;margin-top: -11px;border: 11px solid transparent;display: block;border-left-width: 0;z-index: 99;width: 0;height: 0;}
.private_letter_list_dialog_content{margin-left: 60px;}
.private_letter_list_dialog_content>div{width: 513px;height: 69px;border: 1px solid #ddd;border-radius: 5px;margin-top: 0px;}
.private_letter_list_dialog_content>div>a{font-size: 14px;font-weight: bold;padding-left: 20px;line-height: 30px;}
.private_letter_list_dialog_content>div>span{font-size:14px;}
.private_letter_list_dialog_content>div>label{font-size:12px;padding-left: 20px;opacity: 0.8;}
/*通知页面*/
.notice_main{}
.notice_page{margin-top: 30px;}
.notice_page_list{width: 654px;height: 59px;border-bottom: 1px solid #ddd;margin-left: 50px;margin-top: 10px;}
.notice_page_list>a,.notice_page_list>span{font-size: 14px;}
.notice_page_list>a{color: #428bca;}
.notice_page_list>span{}
.notice_page_list>p{font-size: 12px;opacity: 0.8;padding-top: 5px;}
.notice_topic{font-weight: bold;}












