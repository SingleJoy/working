<script type="text/javascript">
    $(document).ready(function(e){
        $('.testpaper-navbar .nav-pills li').on("click",function(){
            $(this).siblings().removeClass('active');
            $(this).addClass('active');
        })
        $('.testpaper-question-footer .testpaper-question-actions a').on("click",function(){
            var num = $(this).attr('value');
            // console.log(num);
            $(this).hide();
            $(this).siblings('.dis-anay').show();
            if (num == 1) {
                $(this).parent().parent().next(".testpaper-question-analysis").eq(0).show();
            }else if (num == 2) {
                $(this).parent().parent().next(".testpaper-question-analysis").eq(0).hide();
            }
        })
        // 只看错题
        $('#showWrong').on('click',function(){
            if ($(this).is(':checked')) {
                $('.right-question').hide();
            }else{
                $('.right-question').show();
            }
        })
    })
</script>
