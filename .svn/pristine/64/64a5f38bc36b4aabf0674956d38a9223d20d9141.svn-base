    SELECT 
    us.id,us.user_id,us.source_id,us.source_type,us.source_name,us.create_time,
    us.msg,us.learn_id,u.username
    FROM user_study_record us 
    LEFT JOIN user u ON us.user_id=u.id
    WHERE us.learn_id = #{learn_id,jdbcType=BIGINT}
    AND us.source_id = #{source_id,jdbcType=BIGINT}
    AND us.source_type = #{source_type,jdbcType=INTEGER}
    ORDER BY us.create_time DESC
    LIMIT 5
    
  </select>
  <select id="queryUserStudyRecordStati" parameterType="cn.bnsr.edu_yun.backstage.content.view.ContentStatiView" resultType="cn.bnsr.edu_yun.backstage.content.view.ContentStatiView">
  	SELECT count(*) as newNum,days 
	from (
		SELECT  * 
		from ( 
