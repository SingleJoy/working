    <script>

        $(function(){
            $("#all-task").click(function(){
                $(this).addClass("task1");
                $("#my-task").removeClass("task2");
                $(".all-task").show();
                $(".my-task").hide();
            });

            $("#my-task").click(function(){
                $(this).addClass("task2");
                $("#all-task").removeClass("task1");
                $(".my-task").show();
                $(".all-task").hide();
            });
        });
        $(function(){
            $(".task-list>a").click(function(){
                $(".task-list").find("ul").find("li").hide();
                $(this).siblings("ul").show().find("li").toggle();
            })
        });
        $(function(){
            $("#lesson_pic,.choose_pic,.mask").mouseover(function(){
                $(".mask,.choose_pic").show()

            }).mouseout(function(){
                $(".mask,.choose_pic").hide()
