  <select id="querytimesByduaration" parameterType="cn.bnsr.edu_yun.backstage.base.view.UserStatsView" resultType="cn.bnsr.edu_yun.backstage.base.view.UserStatsView">
    SELECT x.interval1,COUNT(*) AS newNum FROM
	(SELECT ud.total_ms,
	 <![CDATA[
	CASE WHEN ud.total_ms<=60000 THEN '1min' 
	WHEN ud.total_ms>60000 AND ud.total_ms<=3*60000 THEN '3min' 
	WHEN ud.total_ms>3*60000 AND ud.total_ms<=10*60000 THEN '10min'
	WHEN ud.total_ms>10*60000 AND ud.total_ms<=30*60000 THEN '30min'
	WHEN ud.total_ms>30*60000 AND ud.total_ms<=60*60000 THEN '1hour'
	WHEN ud.total_ms>60*60000 AND ud.total_ms<=3*60*60000 THEN '3hour'
	WHEN ud.total_ms>3*60*60000 AND ud.total_ms<=10*60*60000 THEN '10hour'
	WHEN ud.total_ms>10*60*60000 THEN 'more than 10hour' END AS interval1]]>
	FROM 
	<if  test="days != '' ">
	user_duration_part
	</if>
	<if  test="startTime != '' and endTime != '' ">
	user_duration
	</if> 
	ud INNER JOIN user u ON ud.user_id=u.id 
	WHERE 1=1
	<if test="status == 1">
	   AND TO_DAYS(u.create_time)=TO_DAYS(ud.end_time)
	</if>
	<if test="status == 2">
	   AND TO_DAYS(u.create_time)!=TO_DAYS(ud.end_time)
	</if>
	<if test="province_id != null and province_id != '' and province_id!='0'.toString()">
	    AND ud.province_id = "${province_id}"
	</if>
	<if  test="startTime != '' and endTime != '' ">
	 	<![CDATA[ AND ud.status=1 AND ud.start_time >= '${startTime}' AND ud.end_time < '${endTime}' ]]>  
	</if>
	<if  test="days != '' ">
		<![CDATA[AND ud.end_time LIKE '${days}%' ]]>  
	</if>) x
	GROUP BY x.interval1 ORDER BY newNum DESC
  
  </select>
  <select id="querylastTimeOnlineInterval" parameterType="cn.bnsr.edu_yun.backstage.base.view.UserStatsView" resultType="cn.bnsr.edu_yun.backstage.base.view.UserStatsView">
    SELECT z.interval1,COUNT(*) AS newNum FROM (                        
	SELECT <![CDATA[
	CASE WHEN y.interval0>=0 AND y.interval0<=1 THEN '1 day'
	WHEN y.interval0>1 AND y.interval0<=2 THEN '2 day'
	WHEN y.interval0>2 AND y.interval0<=7 THEN '7 day'
	WHEN y.interval0>7 AND y.interval0<=15 THEN '15 day'
	WHEN y.interval0>15 AND y.interval0<=30 THEN '30 day'
	WHEN y.interval0>30 THEN 'more than 30 day' ELSE 'first' END ]]>  AS interval1 FROM (
	SELECT CASE WHEN COUNT(*)=1 THEN -1 ELSE DATEDIFF(x.start_time,(SELECT end_time FROM user_duration 
	WHERE user_id=x.user_id 
	<if test="status == 1">
	   AND TO_DAYS(start_time)=TO_DAYS(x.start_time)
	</if>
	<if test="status == 2">
	   AND TO_DAYS(start_time)!=TO_DAYS(x.start_time)
	</if>
	<if test="province_id != null and province_id != '' and province_id!='0'.toString()">
	    AND province_id = x.province_id
	</if>
	
	ORDER BY id DESC LIMIT 1,1)) END AS interval0
	FROM (
	SELECT ud.id, ud.user_id,ud.start_time 
	 FROM user_duration ud INNER JOIN user u ON ud.user_id=u.id     
	 WHERE 1=1 
	<if test="status == 1">
	   AND TO_DAYS(u.create_time)=TO_DAYS(ud.start_time)
	</if>
	<if test="status == 2">
	   AND TO_DAYS(u.create_time)!=TO_DAYS(ud.start_time)
	</if>
	<if test="province_id != null and province_id != '' and province_id!='0'.toString()">
	    AND ud.province_id = "${province_id}"
	</if>
	<if  test="startTime != '' and endTime != '' ">
	 	<![CDATA[AND ud.start_time >= '${startTime}' AND ud.start_time < '${endTime}' ]]>  
	</if>
	<if  test="days != '' ">
		<![CDATA[AND ud.start_time LIKE '${days}%' ]]>  
	</if>
	 ORDER BY ud.id DESC )x GROUP BY x.user_id )y WHERE y.interval0 IS NOT NULL) z GROUP BY interval1 ORDER BY newNum DESC
  
  </select>
