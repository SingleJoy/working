.webuploader-container {
	position: relative;
}
.webuploader-element-invisible {
	position: absolute !important;
	clip: rect(1px 1px 1px 1px); /* IE6, IE7 */
    clip: rect(1px,1px,1px,1px);
}
.webuploader-pick{
	position: relative;
	display: inline-block;
	cursor: pointer;
	text-align: center;
	border-radius: 3px;
	overflow: hidden;

}
.webuploader-pick-disable {
	opacity: 0.6;
	pointer-events:none;
}

