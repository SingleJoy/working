.lesson_list{margin-bottom: 20px;}
.lesson_content_title{font-weight:bold;border-left: 3px solid #46c37b;padding-left: 18px;}
.lesson_content_page{background-color:#f4f4f4;padding-left: 10px;}
.lesson_content_page>label{margin-top: 20px;padding-left: 15px;}
.lesson_content_text{padding-left: 20px;line-height: 2;min-height:40px;}

.list-group,.list-group-item{width: 258px;}
.list-group>li.active{border-left: 2px solid #46c37b;}
.course-manage-header{background-color: #fff;margin-top: 20px;}
#myNav{top:10px;}
.square{width:50px;height: 50px;background-color: #ce1c00;text-align: center;cursor: pointer;position: relative;z-index: 99;}
.square>span{font-size: 16px;color: #fff;line-height: 50px;text-align: center;display: inline-block;}
.arrow_down{ width: 0;  height: 0;  border-left: 25px solid transparent;
    border-right: 25px solid transparent;  border-top:25px solid #ce1c00; }
.isosceles_right_triangle_left{width: 0;height: 0;border-bottom: 28px solid #ce1c00;border-right: 29px solid transparent;display: inline-block;}
.isosceles_right_triangle_right{width: 0;height: 0;border-bottom: 28px solid #ce1c00;border-left: 28px solid transparent;display: inline-block;}
.teaching_plan_name{font-size: 20px;margin: 35px 0 0 0;}
