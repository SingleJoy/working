<script type="text/javascript">
  $(document).ready(function(e){
      //添加助教
      $(".tutor_add").on("click",function(){
          $('#modal').show();
          $('.modal_tutor').append('<div class="modal-backdrop  in"></div>');
      })
      // 关闭弹窗
      $('.pop_close').click(function(){
          $('#modal').hide();
          $('.modal-backdrop').remove();
      })
      //确定助教
      $('.tutor_confirm').on('click',function(){
          $('input[type="checkbox"]:checked').each(function(){
              // console.log($(this).attr("data-name"));
              var tutor_name = $(this).attr('data-name');
              var str = '<li class="active"><a href="javascript:;">'+tutor_name+' &nbsp;&nbsp;<span class="tutor_del">X</span></a></li>';
              $('.tutor_list').append(str);
          })
          $('#modal').hide();
          $('.modal-backdrop').remove();
      })
      //移除助教
      $('.tutor_del').on("click",function(){
          $(this).parent().parent().remove();
      })
      $('.class_range .nav-pills li').on("click",function(){
          $(this).siblings().removeClass('active');
          $(this).addClass('active');
      })
  })
</script>
