<script type="text/javascript">
  $(document).ready(function(e){
      // 预览
      $(".show_pop").on("click",function(){
          $('#modal').show();
          $('.modal-open').append('<div class="modal-backdrop  in"></div>');
      })
      // 关闭弹窗
      $("#modal").on("click",".pop_close",function(){
          $('#modal').hide();
          $('.modal-backdrop').remove();
      })
      // 全选
      $(".checkAll").click(function () {
          var isChecked = $(this).prop("checked");
          $(".subCheckbox").prop("checked", isChecked);
      })
      //删除选中
      $('.del-sel').click(function(){
          // var num = $('.nav-pills .active').attr('value');
          $('.sel-div input:checked').each(function(i,ck){
              $(ck).parent().parent().remove();
              // if (num == 6) {}
          })
          $(".checkAll").prop("checked", false);
      })
      $('.del-question').click(function(){
          $(this).parent().parent().parent().remove();
      })
      // 选择题型
      $('.nav-pills li').on("click",function(){
            $(this).siblings().removeClass('active');
            $(this).addClass('active');
            var num = $(this).attr('value');
            $('#testpaper-table tbody').eq(num).removeClass('hide');
            $('#testpaper-table tbody').eq(num).addClass('sel-div');
            $('#testpaper-table tbody').eq(num).siblings().addClass('hide');
            $('#testpaper-table tbody').eq(num).siblings().removeClass('sel-div');
            $('.testpaper-thead').removeClass('hide');
      })
      // 添加试题
      $('.exam-add-question').on("click",function(){
            $('#modal').html('');
            $('#modal').append('<div class="modal-dialog  modal-lg"><div class="modal-content"><div class="modal-header"><button type="button" class="close pop_close">×</button><h4 class="modal-title">试卷添加题目</h4></div><div class="modal-body"><form class="form-inline well well-sm" action=""><div class="form-group"><select class="form-control" name="target" style="width:200px;height:32px;"><option value="">--按从属--</option><option value="">本课程</option><option value="">课时1： 111</option></select></div><div class="form-group"><input type="text" id="keyword" name="keyword" class="form-control" value="" placeholder="关键词"></div><button class="btn btn-primary btn-sm">搜索</button></form><table class="table table-condensed"><thead><tr><th width="45%">题干</th><th>类型</th><th>分值</th><th>操作</th></tr></thead><tbody><tr><td>测试试卷添加试题<br><small class="text-muted">从属：本课程</small></td><td>单选题</td><td>21.0</td><td><button class="btn btn-default btn-sm newWindowPreview">预览</button><button type="button" class="btn btn-primary btn-sm">选择</button></td></tr></tbody></table></div><div class="modal-footer">    <button type="button" class="btn btn-default pull-right pop_close" >关闭</button></div></div></div>');
            $('#modal').show();
            $('.modal-open').append('<div class="modal-backdrop  in"></div>');
      })
      // 替换试题
      $('.exam-replace-question').on("click",function(){
            $('#modal').html('');
            $('#modal').append('<div class="modal-dialog  modal-lg"><div class="modal-content"><div class="modal-header"><button type="button" class="close pop_close">×</button><h4 class="modal-title">试卷添加题目</h4></div><div class="modal-body"><form class="form-inline well well-sm" action=""><div class="form-group"><select class="form-control" name="target" style="width:200px;height:32px;"><option value="">--按从属--</option><option value="">本课程</option><option value="">课时1： 111</option></select></div><div class="form-group"><input type="text" id="keyword" name="keyword" class="form-control" value="" placeholder="关键词"></div><button class="btn btn-primary btn-sm">搜索</button></form><table class="table table-condensed"><thead><tr><th width="45%">题干</th><th>类型</th><th>分值</th><th>操作</th></tr></thead><tbody><tr><td>测试试卷添加试题<br><small class="text-muted">从属：本课程</small></td><td>单选题</td><td>21.0</td><td><button class="btn btn-default btn-sm newWindowPreview">预览</button><button type="button" class="btn btn-primary btn-sm">选择</button></td></tr></tbody></table></div><div class="modal-footer">    <button type="button" class="btn btn-default pull-right pop_close" >关闭</button></div></div></div>');
            $('#modal').show();
            $('.modal-open').append('<div class="modal-backdrop  in"></div>');
      })
      // 保存试卷  
      $('.request-save').on("click",function(){
            $('#modal').html('');
