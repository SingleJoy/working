<script type="text/javascript">
$(document).ready(function(e){
    var sum = $('.panel-body .nav-tabs>.active').attr('value');
    $('.panel-body .nav-tabs>li').mouseover(function(){
        var num = $(this).attr('value');
        if (num == 1) {
            $('.highlight').remove();
            $('.panel-body .nav-tabs').append('<li class="highlight" style="left: 0px; width: 108px; overflow: hidden;"></li>');
            $(this).addClass('active');
        }else if (num == 2) {
            $('.highlight').remove();
            $('.panel-body .nav-tabs').append('<li class="highlight" style="left: 108px; width: 108px; overflow: hidden;"></li>');
            $(this).addClass('active');
        }
    }).mouseout(function(){
        $('.panel-body .nav-tabs>li').removeClass('active');
        $('.panel-body .nav-tabs>li').each(function(i,ck){
            var sn = $(ck).attr('value');
            if (sn == sum ) {
                $('.highlight').remove();
                if (sn == 1) {
                    $('.panel-body .nav-tabs').append('<li class="highlight" style="left: 0px; width: 108px; overflow: hidden;"></li>');
                }else if (sn == 2) {
                    $('.panel-body .nav-tabs').append('<li class="highlight" style="left: 108px; width: 108px; overflow: hidden;"></li>');
                }
                $(ck).addClass('active');
            }
        })
    })
})
</script>

