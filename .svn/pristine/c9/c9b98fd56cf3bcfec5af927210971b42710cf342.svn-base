  <select id="queryCourseFile" resultType="cn.bnsr.edu_yun.frontstage.train.view.CourseFileView">
  	SELECT f.*,u.username as userName 
  	FROM course_file f 
  	LEFT JOIN user u ON u.id= f.user_id
  	WHERE f.course_id = #{courseId}
  	AND f.user_id = #{userId}
  	and f.type=0
  </select>
  
