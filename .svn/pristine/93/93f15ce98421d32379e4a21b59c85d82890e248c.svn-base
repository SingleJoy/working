package cn.bnsr.edu_yun.frontstage.xbjy.view;

import java.util.List;

import cn.bnsr.edu_yun.backstage.base.view.PageBase;

public class ProblemStudyView  extends PageBase{

	private long id;
	private long lesson_id;
	private long parent_id;
	private String comment;//评论内容
	private long user_id;
	private String update_time;
	private int purview;//课例权限标记
	private String username;
	private String icon;
	private Long root_id;
	private String other_username;
	
	private List<ProblemStudyView> sonView;
	
	public long getId() {
		return id;
	}
	public void setId(long id) {
		this.id = id;
	}
	public long getLesson_id() {
		return lesson_id;
	}
	public void setLesson_id(long lesson_id) {
		this.lesson_id = lesson_id;
	}
	public long getParent_id() {
		return parent_id;
	}
	public void setParent_id(long parent_id) {
		this.parent_id = parent_id;
	}
	public String getComment() {
		return comment;
	}
	public void setComment(String comment) {
		this.comment = comment;
	}
	public long getUser_id() {
		return user_id;
	}
	public void setUser_id(long user_id) {
		this.user_id = user_id;
	}
	public String getUpdate_time() {
		return update_time;
	}
	public void setUpdate_time(String update_time) {
		this.update_time = update_time;
	}
	public int getPurview() {
		return purview;
	}
	public void setPurview(int purview) {
		this.purview = purview;
	}
	public String getUsername() {
		return username;
	}
	public void setUsername(String username) {
		this.username = username;
	}
	public String getIcon() {
		return icon;
	}
	public void setIcon(String icon) {
		this.icon = icon;
	}
	public List<ProblemStudyView> getSonView() {
		return sonView;
	}
	public void setSonView(List<ProblemStudyView> sonView) {
		this.sonView = sonView;
	}
	public Long getRoot_id() {
		return root_id;
	}
	public void setRoot_id(Long root_id) {
		this.root_id = root_id;
	}
	public String getOther_username() {
		return other_username;
	}
	public void setOther_username(String other_username) {
		this.other_username = other_username;
	}
	
}
