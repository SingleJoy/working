<script >
//    tab选项卡添加下划线样式
    $(function(){
        $(".nav-btn-tab>ul>li.active").addClass("height-light").css("border-bottom","2px solid #46c37b");
        $(".nav-btn-tab>ul>li").mouseenter(function(){
            $(this).css("border-bottom","2px solid #46c37b");
        }).mouseout(function(){
            $(this).css("border","none");
            $(".nav-btn-tab>ul>li.active").addClass("height-light").css("border-bottom","2px solid #46c37b");
        });

    });

//班次选择
    $(function(){
        $("#class-list>button").click(function(){
            $(this).addClass("disabled").siblings().removeClass("disabled");
        })
    });

//阶段显示与隐藏
$(function(){
    $(".course-show").click(function(){
       if($(this).parent().parent().parent().siblings().css("display")=="none"){
           $(this).parent().parent().parent().siblings().slideDown(300);
           $(this).find("i").addClass("es-icon-keyboardarrowdown").removeClass("es-icon-keyboardarrowup");
       }else{
           $(this).parent().parent().parent().siblings().slideUp(300);

           $(this).find("i").addClass("es-icon-keyboardarrowup").removeClass("es-icon-keyboardarrowdown");

       }
    })
});


</script>
