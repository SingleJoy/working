/*main主内容*/
.main{width: 1000px;height:100px;position: relative;margin:100px auto;background-color: #fff;}
.main_title{font-size: 16px;position: absolute;margin-top: -40px;color: #3ebbce;}
.add_some_info{position: relative;}
.choose_file,.add_inf,.success_upload{width: 50px;height:50px;border-radius: 50%;font-size: 18px;color:#000;
text-align: center;line-height: 50px;margin-left: 100px;position: absolute;margin-top: 25px;}
.choose_file{background-color: #3ebbce;color: #fff;}
.choose_file p{color: #3ebbce;}
.choose_file p ,.add_inf p,.success_upload p {font-size: 12px;margin-top: -15px;}
.upload_progress_1,.upload_progress_2{width:300px;height:2px;margin-top: -60px;margin-left: 50px;}
.upload_progress_2{margin-top: -60px;}
.add_inf{margin-left: 450px;}
.success_upload{margin-left: 800px;}
.upload_progress{margin-top: -50px;}
