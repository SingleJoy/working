   <sql id="myCollect">
	   <if test="source_type == 0 " >
		  	LEFT JOIN course c ON  c.id = s.learn_id
		  	LEFT JOIN course_info info ON  info.course_id = c.id
		  	LEFT JOIN user u ON u.id = c.user_id
			LEFT JOIN user_info ui ON u.id=ui.user_id 
	   	</if>
	 	<if test="source_type == 1 " >
	 		LEFT JOIN train t ON t.id = s.learn_id
	 	</if>
	 	<if test="source_type == 2 " >
	 		LEFT JOIN lesson l ON l.id = s.source_id
	 	</if>
	  	
	 	WHERE s.user_id = #{userId,jdbcType=BIGINT}
	 	AND s.source_type=#{source_type,jdbcType=INTEGER}
   </sql>
   <select id="queryMyCollect" parameterType="cn.bnsr.edu_yun.frontstage.train.view.UserCollectView" resultType="cn.bnsr.edu_yun.frontstage.train.view.UserCollectView">
    SELECT
   	<if test="source_type == 0 " >
   		 c.id, (CASE WHEN info.is_default_img=0 THEN (SELECT path FROM cert_model_picture WHERE type= 3 AND defaulted = 0 order by upload_time desc limit 1) ELSE info.img END) as img, c.title, info.price, s.source_type
   		 ,u.username as userName,ui.school
   	</if>
   	<if test="source_type == 1 " >
   		t.id, (CASE WHEN t.is_default_img=0 THEN (SELECT path FROM cert_model_picture WHERE type= 4 AND defaulted = 0 order by upload_time desc limit 1) ELSE t.img END) as img, t.`name` AS title, t.price, s.source_type,t.sponsor as userName
   	</if>
   	<if test="source_type == 2 " >
   		l.id, (CASE WHEN l.is_default_img=0 THEN (SELECT path FROM cert_model_picture WHERE type= 5 AND defaulted = 0 order by upload_time desc limit 1) ELSE l.lesson_pic END) as img, l.`name` AS title,  s.source_type
   	</if>
  	FROM user_collect s 
  	<include refid="myCollect"></include>
  	ORDER BY s.collect_time desc
  	<if test="rows!=null and rows!=0">
	LIMIT ${startLine},${rows}
	</if>
  </select>
