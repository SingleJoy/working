/*讨论区话题*/
.discuss_topic_page{margin-top: 20px;margin-left: 15px;}
.discuss_topic_page>p{width: 760px;height:36px;background-color: #eaeaea;font-size: 14px;line-height:36px;padding-left:15px; }
.discuss_topic_page>a>img{width: 40px;height:40px;border-radius: 5px;float: right;margin-top: -30px;margin-right: 20px;}
.discuss_topic_page>h2{clear: both;font-size: 20px;line-height: 36px;font-weight: normal;margin-top: 10px;}
.discuss_topic_page>h3,.discuss_topic_list>h3{font-size: 20px;line-height: 36px;font-weight: normal;margin: 30px 0;}
.discuss_topic_page>div{clear: both;}
.discuss_topic_page>div>span{font-size:12px;padding: 0 5px;opacity: 0.8;}
.discuss_topic_page>label{font-size:14px;margin-top: 20px;display: block;}
.discuss_topic_page>li{margin-top: 30px;}
.discuss_topic_page>li>a{font-size:14px;display: inline-block;height: 34px;width: 90px;text-align: center;line-height: 34px;color: #3ebbce;}
.discuss_topic_list>ul{margin-top:10px;border-top:2px solid #ddd;width: 770px;}
.discuss_topic_list>ul>li{height: 100px;border-bottom: 1px solid #ddd;}
.discuss_topic_list>ul>li>a{}
