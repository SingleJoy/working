
/*open_class_detail_top*/
.open_class_detail_top{width: 1100px;margin: 20px auto;height:280px;background-color: #ffffff;position: relative;}
.open_class_detail_img{width: 300px;height: 180px;margin:15px;}
.open_class_detail_page{margin-left:350px;margin-top: -200px;}
.open_class_detail_page>p{font-size: 22px;}
.open_class_detail_page>span{font-size: 14px;display: block;margin: 10px 0;}
.open_class_detail_page>label{font-size: 12px;margin-left: 10px;}

.open_class_detail_top_right{width: 225px;height:200px;margin-left: 875px;background-color:#f0f0f0;position: absolute;}
.open_class_detail_top_right>img{width: 25px;height: 25px;position: absolute;}

.admin_img{margin-left: 85px;margin-top: 20px;}
#collect,#share,#study_out{width:70px;height: 60px;display: inline-block;float: left;text-align: center;font-size: 14px;}
#collect img,#share img,#study_out img{cursor: pointer;width: 25px;height: 25px;}
#share{margin-left:50px;}
#share span{font-size: 14px;}
.open_class_detail_top_right>label{font-size: 22px;margin-left: 110px;margin-top: 20px;display: inline-block;}

#study_out{margin-left: 10px;}


.learning_progress{margin-top: 20px;position:relative;}
.learning_progress p{font-size: 12px;margin-left: 15px;float: left;}
.learning_progress p span{padding-left:15px; }
.progress_bar{width: 800px;height:11px;border-radius: 5px;background-color: #3ebbce;}
.next_class{font-size: 14px;opacity: 0.8;margin-left: 640px; margin-top: -16px;width:300px;}
.continue_learning{width: 118px;height: 38px;border-radius: 5px;text-align: center;font-size: 16px;position: absolute;margin-left: 940px;line-height: 40px;color: #fff;margin-top: 230px;background-color: #3ebbce;z-index: 99;}

/*open_class_detail_main*/
.open_class_detail_main{width: 1100px;margin:0 auto;height: auto;}

.open_class_detail_main_left{width: 800px;background-color: #fff;float: left;min-height: 280px;margin-bottom: 40px;}

.open_class_detail_tabs{}
.open_class_detail_tabs>p{font-size: 16px;color:#3ebbce;display: inline;float: right;margin-top: -25px;margin-right: 40px;cursor: pointer;}
.open_class_detail_tabs ul{margin-left: 15px;margin-top: 20px;border-bottom: 1px solid #ddd;padding-bottom: 15px;width: 770px;}
.open_class_detail_tabs ul li{display: inline;width:75px;margin-right: 15px;}
.open_class_detail_tabs ul li.curr{color: #3ebbce;border-bottom: 2px solid #3ebbce;padding-bottom: 15px;}
.open_class_detail_tabs ul li.curr_01{color: #3ebbce;border-bottom: 2px solid #3ebbce;padding-bottom: 15px;}
.open_class_detail_tabs ul li a{font-size: 16px;padding:20px;}
.open_class_detail_tabs ul li a small.curr{color: #3ebbce;}
.open_class_detail_tabs ul li a small.curr_01{color: #3ebbce;}
.open_class_detail_tabs ul li a.curr{color: #3ebbce;}
.open_class_detail_tabs ul li a.curr_01{color: #3ebbce;}


/*open_class_notice +公告*/
.open_class_notice{width: 600px;height: 677px;border: 1px solid #ccc;background-color:#ffffff;position: fixed;border-radius: 5px;margin-top:-370px;z-index: 9999;}
.open_class_notice>p{font-size: 16px;border-bottom: 1px solid #ccc;margin-top: 15px;padding-left: 15px;padding-bottom: 15px;}
.open_class_add_notice{margin-top: 20px;}
.open_class_add_notice ul{margin-left: 10px;}
.open_class_add_notice ul li{display: inline;padding-bottom: 15px;width:75px;}
.open_class_add_notice ul li.curr{color: #3ebbce;border-bottom: 1px solid #3ebbce;}
.open_class_add_notice ul li a{font-size: 16px;}
.open_class_add_notice ul li a.curr{color: #3ebbce;}

.notice_title{font-size:14px;margin-bottom: 5px;font-weight: bold;margin-left: 40px;margin-top: 30px;float: left;}

.notice_release_time{margin-top: 5px;}
.notice_release_time p{font-size:14px;margin-bottom: 5px;font-weight: bold;margin-left: 40px;margin-top: 30px;display: inline;}
.release_time_input{width: 310px;height: 35px;border: 1px solid #ccc;border-radius: 3px;margin-left: 18px;margin-bottom: 30px;}
.release_notices_to_stu{width: 16px;height: 16px;clear: both;margin-left:115px;position: absolute;margin-top: 5px;}
.notice_release_time span{font-size: 14px;font-weight: bold;line-height: 16px;clear: both;margin-left: 140px;}
/*私信*/
.send_private_messages_popup{width: 600px;height: 330px;border: 1px solid #ddd;border-radius: 5px;position: absolute;background-color: #fff;z-index: 999;margin-top: -800px;}
.send_private_messages_popup>p{font-size: 16px;border-bottom: 1px solid #ccc;padding: 15px;}
.send_private_messages_popup>button{font-size: 20px;margin-left: 560px;background-color: #fff;margin-top: -40px;position: absolute;opacity: 0.8;}
.send_private_messages_popup>label{font-size: 14px;margin-left: 50px;position: absolute;font-weight: bolder;margin-top: 25px;}
.send_private_messages_popup>input{width: 370px;height: 32px;border: 1px solid #ddd;border-radius: 5px;margin-left: 120px;margin-top: 20px;}
.send_private_messages_popup>span{font-size:14px;margin-left: 60px;margin-top: 30px;position: absolute;display: block;font-weight: bolder;}
.send_private_messages_popup>textarea{font-size:14px;margin-left: 120px;margin-top: 30px;position: absolute;width: 378px;height: 72px;border-radius: 5px;resize: none;border: 1px solid #ddd;}
.send_private_messages_popup>div>button{font-size:15px;margin-left: 115px;margin-top: 140px;width: 76px;height: 32px;border-radius: 30px;border: 1px solid #3ebbce;text-align: center;color: #3ebbce;background-color: #fff;}
.send_private_messages_popup>div>button:hover{color: #fff;background-color: #3ebbce;}
/*class_hour*/
.class_hour{margin-top: 30px;}
.class_hour>p{font-size: 16px;border-left: 2px solid #3ebbce;padding-left: 10px;margin-left: 15px;margin-bottom: 20px;}

.period_lesson>ul{margin-top: 10px;margin-left: 10px;}
.period_lesson>ul>li{line-height: 40px;padding-left: 50px;width: 730px;margin-bottom: 10px;}
.period_lesson>ul>li:hover{background-color: #f3fae3;}
.period_lesson>ul>li>a{font-size: 14px;color:#616161;}
.period_lesson>ul>li>a>span{font-size: 14px; padding:0 15px;}
.period_lesson>ul>li>label{font-size: 20px;cursor: pointer;float: right;}
.period_lesson>ul>li>a>b{font-size: 14px;cursor: pointer;font-weight: normal;float: right;padding-right: 20px;opacity: 0.6;}
.period_lesson>ul>li>a>img{width: 16px;height: 16px;float:right;margin-top: 11px;}
.chapter_list{background-color: #f5f5f5;}
.section_list{display: none;}
.class_hour_info{width: 730px;height: 40px;line-height: 40px;margin-left: 10px;}
.class_hour_info:hover{background-color: #f5f5f5;}
.class_hour_info span{font-size: 14px;margin-left:45px;background-color: #e67e22;color: #fff;padding: 2px 5px;}
.class_hour_info p{display: inline;font-size: 14px;float: right;margin-right: 20px;}
.class_hour_info label{display: inline;font-size: 14px;margin-left: 20px;}
#thread_title {display:block;width:770px;height: 34px;padding: 6px 12px;font-size:14px;color:#9e9e9e;
    background-color: #fff;border: 1px solid #ccc;border-radius: 4px;margin-left: 15px;}

#question_quit{font-size: 14px;color: #3ebbce;margin-left: 670px;cursor: pointer;}
#cke_1_resizer,#cke_1_path{display: none;}
.cke_chrome{margin-left: 15px;}
.label-primary {background-color:#37b97d;}
.label-danger{background-color: #e74c3c;}
.label{display:inline;padding: .2em .6em .3em;font-size:75%;font-weight:bold;color:#fff;text-align:center;white-space:nowrap;vertical-align:baseline;border-radius:.25em;}

/*discussion_area */
.discussion_area{position:relative;margin-top: 40px;}
.discussion_area>ul{margin-left: 20px;}
.discussion_area>ul>li{width: 55px;float: left;text-align: center;border-radius: 40px;margin-right: 10px;margin-left: 2px;border:1px solid #ccc;line-height:30px;}
.discussion_area>ul>li.curr{background-color: #3ebbce;}
.discussion_area>ul>li:hover{color: #fff;border-color: #3ebbce;}
.discussion_area>ul>li>a{font-size: 14px;}
.discussion_area>ul>li>a.curr{color: #fff;}
.discussion_area>ul>li>a:hover{color: #3ebbce;}
.discussion_area_sort{margin-left:240px; }
.discussion_area_sort p{font-size: 14px;line-height: 40px;cursor: pointer;}
.discussion_area_sort p:hover{color: #3ebbce;}
.caret{display: inline-block;width: 0;height: 0;margin-left: 5px;vertical-align: middle;border-top: 4px dashed;border-right: 4px solid transparent;border-left: 4px solid transparent;}
.discussion_area_sort ul{display: none;position: absolute;background-color: #fff;z-index:999;border-radius: 5px;border:1px solid #ddd;}
.discussion_area_sort ul li{line-height:40px;width: 120px;text-align: center;}
.discussion_area_sort ul li.curr{background-color: #3ebbce;}
.discussion_area_sort ul li:hover{background-color: #3ebbce;}
.discussion_area_sort ul li a{font-size: 14px;}
.discussion_area_sort ul li a.curr{color: #fff;}
.discussion_area_sort ul li a:hover{color: #fff;}
.release_topic{border: 1px solid #3ebbce;border-radius: 30px;text-align: center;color: #3ebbce;font-size: 14px;background-color: #fff;position: absolute;z-index: 99;padding: 5px 18px;}
.release_topic{margin-left: 690px;margin-top: -30px;}
.release_topic:hover{color: #fff;background-color: #3ebbce;}


/*讨论区域*/
.discussion_area_content{margin-top: 25px;}
.discussion_area_content ul{margin-left: 20px;}
.discussion_area_content>ul>li{height:80px;width:760px;border-bottom: 1px solid #ccc;margin-bottom: 20px;}
.discussion_area_content>ul>li>img{width: 50px;height: 50px;border-radius: 50%;margin: 15px;}
