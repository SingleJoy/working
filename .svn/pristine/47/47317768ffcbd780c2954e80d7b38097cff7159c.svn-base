.discuss_topic_list>ul>li>a>label{position: absolute;font-size: 14px;margin-left:15px;margin-top: 34px;}
.discuss_topic_list>ul>li>p{font-size: 12px;position: absolute;margin-left: 120px;margin-top: -36px;}
.discuss_topic_list>ul>li>span{padding: 0 3px;margin-left: 50px;margin-top:33px;position: absolute;}
.discuss_topic_list>ul>li>label{font-size: 12px;margin-left: 10px;}
.discuss_topic_list>button{font-size: 14px;color: #3ebbce;border: 1px solid #3ebbce;background-color: #fff;text-align: center;border-radius: 30px;float: right;padding: 6px 24px;margin-bottom:30px;margin-right: 20px;}
