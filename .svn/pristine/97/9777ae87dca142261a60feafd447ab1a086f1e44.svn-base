}

.balloon-filelist ul li {
    position: relative;
    border-bottom: 1px solid #ddd;
    padding: 8px 10px;
}

.balloon-dnd {
    visibility: hidden;
}

.balloon-uploader-none .balloon-uploader-footer,
.balloon-uploader-none .balloon-uploader-body {
    visibility: hidden;
}

.balloon-uploader-none .balloon-dnd {
    visibility: visible;
