* {margin:0;padding:0;text-decoration:none;color: #6d6d6d;}
body {background-color:#eaeaea;font-family:"Microsoft YaHei";}
li{list-style:none;}
a{text-decoration : none ;}
input,button,select,textarea{outline:none;box-shadow: none;}
img,button{outline:none;box-shadow: none;border: none;}
button{text-align: center;}
select{border: 1px solid #ddd;}
table{border-spacing:0; }

.qiuzi_logo{position: absolute;margin-top: 7px;}
.navbar_page{  width: 100%;  height: 45px;  background-color: #3ebbce; position: relative;z-index: 9; }
.navbar{width: 1000px;height: 45px;margin: 0 auto;}
.navbar_ul{position: relative;margin-left: 180px;}
.navbar_ul li {position: relative;float:left;margin-left:45px;line-height: 45px;}
.home{margin-left: 220px;}
.navbar_ul li a{ font-size: 14px; color: #fff;}
.nav_content ul li{font-size: 14px;}


.nav_txt_span{color: #fff;padding-left:10px;}
.nav_title{font-size: 14px;line-height: 50px;position: absolute;margin-top: -454px;background-color: #fff;margin-left: 20px;}

.nav_title img{width: 18px;height:18px;position: absolute;margin-top: 16px;padding-right: 50px;}

/*导航栏-navbar_bottom*/

.navbar_bottom{background-color: #f5f5f5;width: 100%;height: 55px;z-index: -1;}
.navbar_bottom_content{width:1000px;}
.navbar_bottom_content ul li{font-size: 14px;color: #6d6d6d;display: inline;line-height: 55px;}
.navbar_bottom_content ul li a{font-size: 14px;color: #6d6d6d;}
#searchbox{font-size: 12px;width: 20px;height: 60px;position: absolute;margin-top: 12px;z-index: 9999;}
#kw{font-size: 12px;padding-left: 50px;height: 30px;border-radius: 4px;margin-left: 15px;border: 1px solid #ddd;line-height: 30px;}
.submit-btn {
    background:  url("../images/search.png") no-repeat scroll 3px -52px;
    border: 0 none;height: 20px;  left: 210px;  line-height: 99px;
    overflow: hidden; position: absolute;
    top: 6px;  width: 20px;margin-left: -180px;
}
.navbar p{margin-left: 1025px;position: relative;width: 120px;}
.navbar p a{font-size: 14px;color: #fff;line-height:45px;}
.navbar_my_name img{position: absolute;margin-top: 20px;}
.navbar_sign{height: 45px;margin-top: -10px;}
.navbar_sign img{padding-right: 10px;position: absolute;margin-top: 15px;margin-left: -25px;}
.navbar_login{padding-left:45px;height: 45px;}
.navbar_login img{padding-right:10px;position: absolute;margin-top: 15px;margin-left: -25px;}

.navbar_my_name{font-size: 12px;position: absolute;line-height:45px;color: #fff;width: 150px;text-align: right;margin-left:930px;}
.navbar_my_name img{padding-left: 5px;cursor: pointer;border: none;position: absolute;margin-top:13px;}
.admin_states{font-size: 12px;margin-top:29px;width: 100px;height: 90px;background-color: #f8f8f8;position:absolute;z-index: 9999;float: right;margin-left:1000px;display: none;}
.admin_states ul li{margin-top: -10px; }
.admin_states ul li{font-size: 12px;text-align: center;margin-top:2px;color: #ffffff;height:20px;line-height: 16px; }
.admin_states ul li:first-child{margin-top:4px;color: #fff;padding-left: 7px;}
.admin_states ul li a{color: #6d6d6d;}
.sign_out{margin-top: -30px;margin-left: -10px;}

/*导航栏消息通知*/
/*.triangle{width: 0;height: 0;border-left: 5px solid #ffffff;border-right: 5px solid #ffffff;border-bottom: 10px solid transparent; position: absolute;background-color: #ffffff;margin-top: -11px;margin-left: 153px;z-index: 999;}*/
.notice_box{width: 338px;height: 330px;border: 1px solid #ececec;position: absolute;margin-top: 15px;z-index: 9999;background-color: #fff;display: none;}
.notice_box p{text-align: center;margin-top: 20px;border-top:1px solid #eaeaea;}
.notice_box p a{cursor: pointer;text-align: center;font-size: 14px;color: #6d6d6d;line-height: 40px;}
.notice_box ul{margin-top: 2px;margin-left: 3px;}
.notice_box ul li{width: 110px;height:45px;float: left;text-align: center;line-height: 45px;border-bottom: 1px solid #eaeaea;}
.notice_box ul li:first-child{background-color: #3ebbce;}
.notice_box ul li a{color: #6d6d6d;font-size: 14px;}
.box_content{width: 348px;height: 220px;background-color: #fff;}
.teaching_plan_content,.concern_news_content,.system_news_content{width: 338px;height: 220px;margin-top: 47px;}


.concern_news_content{display: none;}
.system_news_content{display: none;}
.teaching_plan a{color: #ffffff;}
.notice_page{line-height: 20px;padding: 10px 0; }
.notice_page span{width:3px;height:3px;display:inline-block;position: absolute;}
.notice_page a{color: #2bbb61;display: inline;height: auto;font-size: 12px;margin-left: 15px;}
.dot{width: 3px;height: 3px;background:#3ebbce;position:absolute;margin-top:8px;margin-left: 3px;}


/*评分显示样式 */
.star1{width: 85px;height:17px;background:url('../images/score_star_icon_black.png') repeat-x left center;}
.star2{height:17px;background:url('../images/score_star_icon.png') repeat-x left center;float: left;}

/*尾部 ---footer*/
.footer{width:100%;height: 80px;background-color:#3ebbce;position:relative;margin-top: 100px;z-index: -1;clear: both;}
.footer_content{padding-top:32px;}
.footer_title,.agreement,.proposal{font-size: 14px;color: #fff;}
.footer_content_span{padding-left:20px;padding-right:20px;color:#fff;}
.footer_content ul li{display: inline;}
