/*搜索框*/
.search{width: 400px;margin: 20px auto;}
.search_input{width:198px;height:38px;border: 1px solid black; border-right: none;position: relative;  border-radius: 5px 0 0 5px;  padding-left: 10px;  font-size: 16px;  }
.search_button{width:80px;height:42px;border-left:none;  background-color: #3ebbce;color: #fff;  border-radius: 0 5px 5px 0 ;  margin-left: -5px; position: absolute;margin-top: -2px; }
.search form label{font-size: 12px;}
.search form input{position: relative;margin-left: 5px;margin-top: 10px;  }

.resourse_navbar_bottom{background-color:#fff;width: 100%;height: 55px;z-index: -1; }
.resourse_navbar_bottom>div{width: 1100px;margin: 0 auto;}
.nav_content_classify_ul li{width: 40px;border-radius: 5px;line-height: 20px; float: left;margin-left: 20px;text-align: center;background-color: #e7e7e7;color: #000;list-style-type:none;}
.nav_content_classify_ul li.curr{background-color:#3ebbce;color: #fff;}
.nav_content_classify_ul li>a{font-size: 14px;text-align: center;color: #313131;}
.nav_content_classify_ul>li.curr>a{color: #fff;}
.nav_content_classify{background-color: #fff;width:270px;height: 300px;border:1px solid #ddd;position: absolute;z-index: 9999;margin-top:25px;border-radius: 4px;}
.nav_content_classify_title_name{font-size: 16px;padding-left:24px;border-bottom: 1px solid #ddd;padding-bottom: 10px;margin-top: 10px;}
.nav_content_classify p{font-size: 14px;display: inline;float: left;margin-left: -20px;}
.nav_content_sel_title{font-size: 14px;padding-left: 20px;margin-top: 10px;}
.nav_content_sel{font-size: 12px;margin-top: 10px;margin-left: 27px;width: 140px;height:30px;}
.content_sel_1{margin-left: 10px;}
.nav_content_button{width: 100px;height:30px;border-radius: 5px;background-color: #3ebbce;margin-left:100px;margin-top: 20px;color: #fff;}
.nav_content_img{width: 14px;height:14px;padding-left: 10px;z-index:9999;}

/*主内容区域*/
.main{position: relative;margin: 20px auto;width: 1100px;height: auto;}
/*主内容左边*/

.all_pinyin{width: 200px;height: 46px;text-align: left;padding-left:20px;line-height:50px;border:1px solid #eaeaea;font-size: 14px;}
.all_pinyin a{font-size: 14px;}
.main_left_tab_img{padding-left: 10px;}
.literacy_content_01,.literacy_content_02,.literacy_content_03{display: none;}
.nav_txt_span{color: #3ebbce;}



.recommend_page{position: relative;margin-top: 20px;opacity: 0.6;clear: both;}

/*二级目录最新上传*/
.latest_upload{height: 122px;position: relative;z-index: 1;margin-left: 20px;}
.latest_upload_page{position:relative;margin-top:20px;border: 1px solid #ddd;width: 250px;height:120px;overflow: hidden;float: left;margin-left: -20px;}
.latest_upload_page_content{width:250px;height:58px; background-color: #ffffff; border-bottom: 1px solid #ddd;padding-top: 15px;}
.latest_upload_title{font-size: 14px;margin-top: 10px;}
.latest_upload_title_text{ font-size: 12px;position: relative; color:#2c78a2;padding-left: 30px;padding-top: 15px; }
.upload_formate{font-size: 12px;margin-left: 30px;padding-top: 15px;}
.latest_upload_page_score{}
.upload_author{width:250px;background-color: #fbfbfb;font-size: 12px;opacity: 0.6;overflow:hidden;padding-left: 25px;padding-top: 10px;height: 30px;}
.score_num{color: #fe9a00;padding-left: 10px;}


