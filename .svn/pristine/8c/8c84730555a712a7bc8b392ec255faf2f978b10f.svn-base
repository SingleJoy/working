package cn.bnsr.edu_yun.frontstage.xbjy.view;

public class LessonProblemView {

	private long id;
	private long lesson_id;
	private String plan_que;//计划环节问题研讨
	private String prepare_que;//备课环节问题研讨
	private String class_que;//上课环节问题研讨
	private String assessment_que;//评课环节问题研讨
	
	
	public long getId() {
		return id;
	}
	public void setId(long id) {
		this.id = id;
	}
	public long getLesson_id() {
		return lesson_id;
	}
	public void setLesson_id(long lesson_id) {
		this.lesson_id = lesson_id;
	}
	public String getPlan_que() {
		return plan_que;
	}
	public void setPlan_que(String plan_que) {
		this.plan_que = plan_que;
	}
	public String getprepare_que() {
		return prepare_que;
	}
	public void setprepare_que(String prepare_que) {
		this.prepare_que = prepare_que;
	}
	public String getClass_que() {
		return class_que;
	}
	public void setClass_que(String class_que) {
		this.class_que = class_que;
	}
	public String getAssessment_que() {
		return assessment_que;
	}
	public void setAssessment_que(String assessment_que) {
		this.assessment_que = assessment_que;
	}
	
	
}
