.resources_list_school_level_{margin-left: 50px;}
.resources_list_version_{margin-left: 20px;}
.resources_list_grade_{margin-left: -20px;}
.chaxun{width: 80px;height: 30px;background-color: #3ebbce;color: #ffffff;border-radius: 5px;font-size: 12px;}

/*我的下载*/
