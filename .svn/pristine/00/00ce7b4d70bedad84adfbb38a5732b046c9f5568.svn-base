                        <a href="plan-management.html" class="btn btn-default pull-right">返回</a>
                        创建计划
                    </div>

                    <div class="panel-body">


                        <form class="form-horizontal" action="" method="get"   novalidate="novalidate" >

                            <div class="form-group">
                                <div class="col-md-2 control-label"><label><span class="text-danger">*</span>计划名称:</label></div>
                                <div class="col-md-6 controls">
                                    <input type="text"  class="form-control" >
                                    <div class="help-block" style="display: none;">
                                        <span class="text-danger">请输入计划名称</span>
                                    </div>
                                </div>
                            </div>


                            <div class="form-group">
                                <div class="col-md-2 control-label"><label><span class="text-danger">*</span>学时:</label></div>
                                <div class="col-md-2 controls">
                                    <input type="text"  class="form-control" >
                                    <div class="help-block" style="display: none;">
                                        <span class="text-danger">请输入学时</span>
                                    </div>
                                </div>
                            </div>

                            <div class="form-group">
                                <div class="col-md-2 control-label"><label><span class="text-danger">*</span>学分:</label></div>
                                <div class="col-md-2 controls">
                                    <input type="text"  class="form-control" >
                                    <div class="help-block" style="display: none;">
                                        <span class="text-danger">请输入学分</span>
                                    </div>
                                </div>
                            </div>

                            <div class="form-group">
                                <div class="col-md-2 control-label"><label><span class="text-danger">*</span>开始时间:</label></div>
                                <div class="col-md-2 controls">
                                    <input type="text"  class="form-control" >
                                    <div class="help-block" style="display: none;">
                                        <span class="text-danger">请输入开始时间</span>
                                    </div>
                                </div>
                            </div>

                            <div class="form-group">
                                <div class="col-md-2 control-label"><label><span class="text-danger">*</span>结束时间:</label></div>
                                <div class="col-md-2 controls">
                                    <input type="text"  class="form-control" >
                                    <div class="help-block" style="display: none;">
                                        <span class="text-danger">请输入结束时间</span>
                                    </div>
                                </div>
                            </div>

                            <div class="form-group">
                                <label class="col-md-2 control-label"><span class="text-danger">*</span>状态:</label>
                                <div class="col-md-6 controls radios">
                                    <label><input type="radio" name="state">发布</label>
                                    <label><input type="radio" name="state">未发布</label>

                                </div>
                            </div>

                            <div class="form-group clearfix">

                                <div class="col-md-2 controls col-md-offset-1" >
                                    <button class="btn btn-fat btn-info" type="button">创建</button>
                                </div>

                                <div class="col-md-2 controls">
                                    <button class="btn btn-fat btn-danger" type="button">取消</button>
                                </div>

