	<select id="showWorkshop" resultType="cn.bnsr.edu_yun.frontstage.train.view.WorkshopView" parameterType="cn.bnsr.edu_yun.frontstage.train.view.WorkshopView">
		SELECT w.*,
		p.subjectName as periodName,
		s.subjectName as subjectName,
		u.username as workshop_user,
		(SELECT COUNT(*) FROM recommended r WHERE r.source_id=w.id AND r.source_type=6 AND DATE_SUB(r.valid_time,INTERVAL -1 DAY)>NOW() ) as recommended,
		(SELECT COUNT(*) FROM student_relation sr1 WHERE sr1.source_id=w.id AND sr1.source_type=1) AS user_count
		FROM workshop w
		LEFT JOIN subject_list p ON w.period=p.id
		LEFT JOIN subject_list s ON w.subject=s.id
		LEFT JOIN student_relation sr ON sr.source_id=w.id AND sr.source_type=1 AND sr.role_type=3
		LEFT JOIN USER u ON u.id=sr.user_id
		WHERE 1=1
		<if test="community_id!=null ">
			and w.community_id=${community_id}
		</if>
		<if test="id!=null ">
			and w.id=${id}
		</if>
		<if test="status!=null ">
			and w.status=${status}
		</if>
		<if test="search!=null ">
			and w.name like "${search}"
		</if>
		ORDER BY
		<if test="sort!=null ">
			${sort}
		</if>
		<if test="sort==null ">
			w.create_time
		</if>
		<if test="order!=null ">
			${order}
		</if>
		<if test="order==null ">
			desc
		</if>
		<if test="rows!=null and rows!=0">
			LIMIT ${startLine},${rows}
		</if>
	</select>
