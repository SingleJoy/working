	<select id="workshopDataStats" resultType="cn.bnsr.edu_yun.frontstage.train.view.DataStatsView" parameterType="cn.bnsr.edu_yun.frontstage.train.view.DataStatsView">
		SELECT DATE_FORMAT(w.create_time, '%Y-%m-%d') days, COUNT(w.id) AS newNum, t.countNum
		FROM workshop w
		LEFT JOIN (
		SELECT COUNT(*) as countNum, ww.community_id
		FROM workshop ww
		WHERE ww.community_id = ${id}
		) t ON t.community_id = w.community_id
		WHERE w.community_id = ${id}
		<if test="monthType == 0 or monthType == 1 ">
				<![CDATA[AND w.create_time LIKE  '%${days}%']]>
		</if>
		<if test="monthType == 2 or monthType == 3 ">
			 	<![CDATA[ AND (w.create_time >= ${startTime} or w.create_time <= ${endTime} )]]>
		</if>
		GROUP BY days
	</select>

