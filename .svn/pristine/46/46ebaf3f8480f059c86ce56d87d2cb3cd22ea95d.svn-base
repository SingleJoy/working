		SELECT w.id,w.status,w.img,w.name,(SELECT COUNT(*) FROM student_relation sr1 WHERE sr1.source_id=w.id AND sr1.source_type=1) AS studentNum,
		(SELECT COUNT(*) FROM topic_question t WHERE t.source_id=w.id AND t.source_type=3) AS topicNum
		FROM workshop w
		LEFT JOIN student_relation sr ON sr.source_id=w.id AND sr.source_type=1
		WHERE sr.user_id=${workshop_user_id} 
