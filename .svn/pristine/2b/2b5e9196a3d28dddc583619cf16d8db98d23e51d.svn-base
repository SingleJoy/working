  <!-- 平均访问时长 -->
  <select id="meanVisitTrend" parameterType="cn.bnsr.edu_yun.backstage.base.view.UserStatsView" resultType="cn.bnsr.edu_yun.backstage.base.view.UserStatsView">
	SELECT  
	<choose>
	<!-- 按小时 -->
	<when test="timeUnit == 0">DATE_FORMAT(u.end_time,'%H')</when>
	<!-- 按天 -->
	<when test="timeUnit == 1">DATE_FORMAT(u.end_time,'%Y-%m-%d')</when>
	<!-- 按周 -->
	<when test="timeUnit == 2">DATE_FORMAT(SUBDATE(u.end_time,DATE_FORMAT(u.end_time,'%w')-1),'%Y-%m-%d')</when>
	<!-- 按月 -->
	<when test="timeUnit == 3">CONCAT(DATE_FORMAT(LAST_DAY(u.end_time),'%Y-%m-'),'01')</when>
	</choose>
	AS days,CONCAT(SEC_TO_TIME(FLOOR(SUM(u.total_ms)/(COUNT(u.id)*1000)))) AS interval1
	FROM user_duration_part u
	WHERE 1=1 
	<if test="province_id != null and province_id != '' and province_id!='0'.toString()">
	    AND u.province_id = "${province_id}"
	</if>
	<if test="device != null and device !=-1">
	    AND u.device = ${device}
	</if>
	<if  test="startTime != '' and endTime != '' ">
	 	<![CDATA[ AND u.end_time >= '${startTime}' AND u.end_time < '${endTime}' ]]>  
	</if>
	<if  test="days != '' ">
		<![CDATA[AND u.end_time LIKE  '${days}%']]>  
	</if>
	GROUP BY days;
  </select>
  
