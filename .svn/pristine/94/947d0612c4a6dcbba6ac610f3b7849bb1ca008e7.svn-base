  <select id="queryCommunity" parameterType="cn.bnsr.edu_yun.frontstage.train.view.CommunityView" resultType="cn.bnsr.edu_yun.frontstage.train.view.CommunityView">
  	SELECT IFNULL(t.studentNum,0) AS studentNum,IFNULL(tt.topicNum,0) AS topicNum,c.* 
  	FROM community c 
	LEFT JOIN (
		SELECT COUNT(s.source_id) AS studentNum,s.source_id 
		FROM student_relation s 
		WHERE s.source_type = 0
		GROUP BY s.source_id
		) t ON t.source_id = c.id
	LEFT JOIN	(
		SELECT COUNT(q.source_id) AS topicNum, q.source_id
		FROM topic_question q 
		WHERE q.source_type = 2 
		GROUP BY q.source_id 
		) tt ON tt.source_id = c.id
	LEFT JOIN `area` a on c.area_id=a.id
  	WHERE c.status = 0 
  	<if test="area_id!=null">
  	  and   a.area_id like '${area_id}'
  	</if>
	  	<if test="sign == 2||sign==0">
	  		AND  c.user_id = ${user_id}
	  	</if>
	  	<if test="sign == 1||sign==0">
	  		<choose>
	  			<when test="sign==0">
	  				OR
	  			</when>
	  			<otherwise>
	  				AND
	  			</otherwise>
	  		</choose>
	  		c.id in(
			SELECT s.source_id 
			FROM student_relation s
			WHERE s.user_id = ${user_id}
			 AND s.source_type = 0
		)
	  	</if>
	  	<if test="sort!=null">
	  	  order by studentNum desc
	  	</if>
	<if test="paging!=0">
 		LIMIT ${startLine},${rows}
 	</if>
  </select>
  
