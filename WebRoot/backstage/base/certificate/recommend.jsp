<%@ page language="java" pageEncoding="UTF-8"%>
<%@ taglib prefix="c" uri="http://java.sun.com/jsp/jstl/core"%>

<div align="center" style="padding: 5px;overflow: hidden;">
	<form method="post">
		<table class="tableForm">
			<tr><th>&nbsp;<br/>&nbsp;</th></tr>
			<tr>
				<th >序号:</th>
				<td><input name="seq" class="easyui-numberbox"
					precision="0" min="0" max="10000" size="5" maxlength="5"
					data-options="required:true,missingMessage:'请填写序号'"
					style="width:150px;" placeholder="请输入0-10000的整数" /></td>
			</tr>
			<tr><th>&nbsp;<br/>&nbsp;</th></tr>
			<tr>
				<th>有效时间:</th>
				<td><input name="valid_day" class="easyui-numberbox"
					precision="0" min="1" max="10000" size="5" maxlength="5"
					data-options="required:true,missingMessage:'请填写天数'"
					style="width:150px;" placeholder="请输入天数" />天</td>
			</tr>
		</table>
	</form>
</div>
